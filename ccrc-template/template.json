{
  "template": {
    "homepage": ".",
    "scripts": {
      "start": "npm run format && npm run s",
      "s": "react-app-rewired start",
      "build": "npm run format && npm run b",
      "b": "react-app-rewired build",
      "test": "npm run format && npm run t",
      "t": "react-app-rewired test",
      "test:coverage": "npm run format && npm run c",
      "c": "react-app-rewired test --coverage",
      "format": "prettier --loglevel silent --write {src,public}/**/*.{html,css,scss,js,jsx,ts,tsx}",
      "f": "npm run format"
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "url-search-params": "^1.1.0"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^0.9.1",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "postcss-plugin-px2rem": "^0.8.1",
    "prettier": "^2.0.2",
    "react-app-rewired": "^2.1.5"
  }
}
